    <script src="~/client-side/eaglecoin/js/wow.min.js"></script>
    <script src="~/client-side/eaglecoin/js/jquery.countdown.js"></script>
    <script src="~/client-side/eaglecoin/js/owl.carousel.js"></script>
    <script src="~/client-side/eaglecoin/js/slick.min.js"></script>
    <script src="~/client-side/eaglecoin/js/Chart.js"></script>
    <script src="~/client-side/eaglecoin/js/chart-function.js"></script>
    <script src="~/client-side/eaglecoin/js/jquery.fancybox.min.js"></script>
