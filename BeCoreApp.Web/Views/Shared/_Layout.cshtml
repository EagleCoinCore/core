<body>
    <!--Main Wrapper Start-->
    <div class="wrapper" id="top">
        @await Html.PartialAsync("_Header")
        @RenderBody()
        @await Html.PartialAsync("_Footer")
    </div>
    <!--Main Wrapper End-->
    <script src="~/client-side/eaglecoin/js/jquery.min.js"></script>
